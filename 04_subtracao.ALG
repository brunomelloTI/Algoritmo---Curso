algoritmo "subtracao"
var
   N1, N2: inteiro
   S: inteiro
inicio

    escreval ("Informe um número: ")
    leia (N1)
    escreval ("Informe outro número: ")
    leia (N2)
    S <- N1 - N2
    escreval ("")
    escreval (N1," -",N2, " = x")
    Escreval (N1," -",N2, " =", S)
    escreval ("")
    escreval ("A subtração é",  S)
fimalgoritmo